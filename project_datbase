CREATE TABLE PERSON (
    id INT NOT NULL AUTO_INCREMENT,
    name VARCHAR(255) NOT NULL,
    address VARCHAR(255) NOT NULL,
    login VARCHAR(255),
    password VARCHAR(255),
    PRIMARY KEY(id)
);
CREATE TABLE RESTAURANT (
    id INT NOT NULL AUTO_INCREMENT,
    name VARCHAR(255) NOT NULL,
    address VARCHAR(255) NOT NULL,
    customer_care_contact int not null,
    image VARCHAR(255) NOT NULL,
    PRIMARY KEY(id)
);
